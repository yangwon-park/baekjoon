    "n = int(input())\n",
    "waiting = list(map(int, input().split()))\n",
    "\n",
    "waiting = sorted(waiting)\n",
    "\n",
    "d = [0] * (n+1)\n",
    "\n",
    "d[1] = 1\n",
    "\n",
    "for i in range(2, n+1):\n",
    "    d[i] = d[i-1] + waiting[i-1]\n",
    "    \n",
    "print(sum(d))"
   ]
