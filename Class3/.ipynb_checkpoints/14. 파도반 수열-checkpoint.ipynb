   "source": [
    "T = int(input())\n",
    "\n",
    "for _ in range(T):\n",
    "    n = int(input())\n",
    "    d = [0] * (n+1)\n",
    "    \n",
    "    d[1] = 1\n",
    "    d[2] = 1\n",
    "    d[3] = 1\n",
    "    d[4] = 2\n",
    "    d[5] = 2\n",
    "    \n",
    "    for i in range(6, n+1):\n",
    "        d[i] = d[i-1] + d[i-5]\n",
    "        \n",
    "    print(d[n])"
   ]
  }
