   "source": [
    "n = int(input())\n",
    "\n",
    "d = [0] * (n+1)\n",
    "\n",
    "# if n == 1:\n",
    "#     d[n] = 1\n",
    "# elif n == 2:\n",
    "#     d[n] = 2\n",
    "# else:\n",
    "d[1] = 1\n",
    "d[2] = 2\n",
    "\n",
    "for i in range(3, n+1):\n",
    "    d[i] = d[i-1] + d[i-2]\n",
    "    \n",
    "print(d[n] % 10007)"
   ]
