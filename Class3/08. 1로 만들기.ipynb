   "source": [
    "N = int(input())\n",
    "\n",
    "# DP 테이블 생성\n",
    "d = [0] * (N+1)\n",
    "\n",
    "for i in range(2, N+1):\n",
    "    d[i] = d[i-1] + 1\n",
    "    \n",
    "    if i % 2 == 0:\n",
    "        d[i] = min(d[i], d[i // 2] + 1)\n",
    "        \n",
    "    if i % 3 == 0:\n",
    "        d[i] = min(d[i], d[i // 3] + 1)\n",
    "\n",
    "print(d[N])"
   ]
  }
