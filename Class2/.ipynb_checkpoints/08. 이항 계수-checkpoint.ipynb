   ],
   "source": [
    "n, k = map(int, input().split())\n",
    "\n",
    "# 팩토리얼 구현 함수\n",
    "def _factorial(r):   \n",
    "    if r == 0 or r == 1:\n",
    "        return 1\n",
    "        \n",
    "    return r * _factorial(r-1)\n",
    "\n",
    "# 이항계수 관련 변수들\n",
    "nf = _factorial(n)\n",
    "nk = _factorial(k)\n",
    "nr = _factorial(n-k)\n",
    "\n",
    "print(int(nf / (nk * nr)))"
   ]
  }
