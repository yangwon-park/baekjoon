    "n = int(input())\n",
    "\n",
    "dot = []\n",
    "\n",
    "for _ in range(n):\n",
    "    dot.append(list(map(int, input().split())))\n",
    "    \n",
    "dot.sort(key=lambda x: (x[1], x[0]))\n",
    "\n",
    "for d in dot:\n",
    "    print(d[0], d[1])"
   ]
