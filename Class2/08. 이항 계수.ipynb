   "source": [
    "n, k = map(int, input().split())\n",
    "\n",
    "def _factorial(r):   \n",
    "    if r == 1:\n",
    "        return 1\n",
    "        \n",
    "    return r * _factorial(r-1)\n",
    "\n",
    "nf = _factorial(n)\n",
    "nk = _factorial(k)\n",
    "nr = _factorial(n-k)\n",
    "\n",
    "print(int(nf / (nk * nr)))"
   ]
