   "source": [
    "while True:\n",
    "    n = int(input())\n",
    "\n",
    "    if n == 0:\n",
    "        break\n",
    "        \n",
    "    st = str(n)\n",
    "    # list의 요소를 내장 함수 reversed로 뒤집음\n",
    "    st_list = list(reversed(st))\n",
    "\n",
    "    if st_list == list(st):\n",
    "        print('yes')\n",
    "    else:\n",
    "        print('no')"
   ]
