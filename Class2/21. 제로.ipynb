   "source": [
    "from collections import deque\n",
    "\n",
    "k = int(input())\n",
    "\n",
    "stack = deque()\n",
    "\n",
    "for _ in range(k):\n",
    "    n = int(input())\n",
    "    if n != 0:\n",
    "        stack.append(n)\n",
    "    \n",
    "    if n == 0 and stack:\n",
    "        stack.pop()\n",
    "        \n",
    "print(sum(stack))"
   ]
  }
