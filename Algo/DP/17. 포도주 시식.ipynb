   "source": [
    "n = int(input())\n",
    "\n",
    "wine = [0] + [int(input()) for _ in range(n)]\n",
    "\n",
    "d = [0 for _ in range(10001)]\n",
    "\n",
    "d[1] = wine[1]\n",
    "if n > 1:\n",
    "    d[2] = wine[2] + d[1]\n",
    "\n",
    "for i in range(3, n+1):\n",
    "    d[i] = max(d[i-1], d[i-2] + wine[i], d[i-3] + wine[i-1] + wine[i])\n",
    "\n",
    "print(max(d))"
   ]
  }
