   "source": [
    "T = int(input())\n",
    "\n",
    "sec = [300, 60, 10]\n",
    "\n",
    "cnt_list = []\n",
    "for s in sec:   \n",
    "    \n",
    "    cnt = 0\n",
    "    while T >= s:\n",
    "        T -= s\n",
    "        cnt +=1\n",
    "        \n",
    "    cnt_list.append(cnt)\n",
    "    \n",
    "if T != 0:\n",
    "    print(-1)\n",
    "else:\n",
    "    for c in cnt_list:\n",
    "        print(c, end = ' ')"
   ]
  }
