   "source": [
    "# 유클리드 호제법 사용\n",
    "x, y = map(int, input().split())\n",
    "X,Y = x, y\n",
    "\n",
    "def gcd(x, y):\n",
    "    \n",
    "    while x != 0:\n",
    "        y %= x\n",
    "        y,x = x, y\n",
    "        \n",
    "    return y\n",
    "\n",
    "_gcd = gcd(x, y)\n",
    "_lcm = (X * Y) // _gcd  \n",
    "\n",
    "print(_gcd)\n",
    "print(_lcm)"
   ]
  }
